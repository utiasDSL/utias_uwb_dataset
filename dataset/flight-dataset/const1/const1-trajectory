(1) const1-log1 (TDOA2)
        cmd_x = radius * np.cos(omega * t)
        cmd_y = radius * np.sin(omega * t)
        cmd_z = HEIGHT 
        
        goCircle(timeHelper, cf, radius=1.25, round_num=6.0, omega=0.3)
        HEIGHT = 1.5
  
(2) const1-log2 (TDOA2)
        cmd_x = radius * np.cos(0.65 * omega * t)
        cmd_y = -1 * radius * np.sin(0.75 * omega * t) * np.cos(0.15 * omega * t)
        cmd_z = HEIGHT - 0.35 * np.sin(0.75 * omega * t) 
        
        goCircle(timeHelper, cf, radius=1.25, round_num=6.0, omega=0.35)
        HEIGHT = 1.5
  
(3) const1-log3 (TDOA2)
        cmd_x = radius * np.cos(0.65 * omega * t)
        cmd_y = 1.25 * radius * np.sin(0.65 * omega * t) * np.cos(0.85 * omega * t)
        cmd_z = HEIGHT - 0.35 * np.sin(0.75 * omega * t) 
        
        goCircle(timeHelper, cf, radius=1.25, round_num=6.0, omega=0.35)
        HEIGHT = 1.5
                
(4) const1-log4 (TDOA2)
        cmd_x = radius * np.cos(0.65 * omega * t)
        cmd_y = radius * np.sin(0.45 * omega * t) 
        cmd_z = HEIGHT - 0.45 * np.sin(0.75 * omega * t) * np.cos(0.7 * omega * t)
        
        goCircle(timeHelper, cf, radius=1.25, round_num=6.0, omega=0.35)
        HEIGHT = 1.5

