        cmd_y = -1 * radius * np.sin(0.75 * omega * t) * np.cos(0.15 * omega * t)
        cmd_z = HEIGHT - 0.5 * np.sin(0.75 * omega * t) 
        
        goCircle(timeHelper, cf, radius=1.5, round_num=6.0, omega=0.35)
        HEIGHT = 1.5
        
